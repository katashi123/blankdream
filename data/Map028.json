parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Eh?! Oh, what?\""]},{"code":231,"indent":0,"parameters":[5,"utsuro_02",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...The day duty diary."]},{"code":401,"indent":0,"parameters":[" It's your turn today, so the teacher...\""]},{"code":231,"indent":0,"parameters":[5,"yuzu_04",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"A-Ahh! That's right!"]},{"code":401,"indent":0,"parameters":[" Thank you very much, Utsuro!\""]},{"code":231,"indent":0,"parameters":[5,"utsuro_02",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Mm. Well, that's all.\""]},{"code":231,"indent":0,"parameters":[5,"yuzu_06",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Ah...\""]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,0,0,10,false]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["...Once again, we couldn't keep talking."]},{"code":401,"indent":0,"parameters":["Though I longed to talk with her more..."]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["...I, Yuzu Hanasaki, was captivated by Utsuro."]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["She was so unlike the other students. A girl of few words,"]},{"code":401,"indent":0,"parameters":["with a mysterious air, long eyelashes, always looking tired..."]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["And her skin and hair were white as snow..."]},{"code":401,"indent":0,"parameters":["It was apparently a kind of albinoism."]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["At first, I thought I just wanted to be her friend,"]},{"code":401,"indent":0,"parameters":["but quickly realized it wasn't that."]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["Even if it was but puppy love,"]},{"code":401,"indent":0,"parameters":["I was certainly in love with her."]},{"code":231,"indent":0,"parameters":[5,"yuzu_10",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...It is strange, isn't it...\""]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,0,0,10,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["But I was troubled."]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["I loved her, but at the same time..."]},{"code":401,"indent":0,"parameters":["I hesitated at the idea of loving a girl."]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["I at least want to be more friendly with her."]},{"code":401,"indent":0,"parameters":["If not as a lover, then as a good friend..."]},{"code":235,"indent":0,"parameters":[5]},{"code":117,"indent":0,"parameters":[3]},{"code":242,"indent":0,"parameters":[3]},{"code":234,"indent":0,"parameters":[1,[0,0,0,0],180,false]},{"code":223,"indent":0,"parameters":[[-255,-255,-255,51],180,true]},{"code":230,"indent":0,"parameters":[60]},{"code":121,"indent":0,"parameters":[76,76,0]},{"code":201,"indent":0,"parameters":[0,27,10,10,8,0]},{"code":0,"indent":0,"parameters":[]}]}],"x":9,"y":7}
