ode":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Once, there was a painter."]},{"code":401,"indent":0,"parameters":[" He was known as the mad genius, Rinzou Usui.\""]},{"code":231,"indent":0,"parameters":[5,"mishiro_04",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Huh? You know my grandpa...?!\""]},{"code":231,"indent":0,"parameters":[5,"yuzu_05",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Wait, Mishiro. It looks like..."]},{"code":401,"indent":0,"parameters":[" Maybe she isn't aware of us?\""]},{"code":231,"indent":0,"parameters":[5,"mishiro_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...\""]},{"code":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Rinzou had a wife, but he also had"]},{"code":401,"indent":0,"parameters":[" a relationship with a servant.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"With his wife, he had a child, Shinji."]},{"code":401,"indent":0,"parameters":[" With the servant, he had a child, Tamaki...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Rinzou's two children, born of different mothers.\""]},{"code":231,"indent":0,"parameters":[5,"mishiro_02",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Huh...?\""]},{"code":231,"indent":0,"parameters":[5,"yuzu_05",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...\""]},{"code":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki was the illegitimate child of an affair."]},{"code":401,"indent":0,"parameters":[" But because Shinji showed no interest in art...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Rinzou's affection was often directed at Tamaki,"]},{"code":401,"indent":0,"parameters":[" who did have an interest in the arts.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki learned art from Rinzou, and given deep love,"]},{"code":401,"indent":0,"parameters":[" grew in the shadow of the Usui family.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"But these irregular days couldn't last.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"When Tamaki turned 12,"]},{"code":401,"indent":0,"parameters":[" Rinzou's wife learned of her existence.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Rinzou divorced his wife,"]},{"code":401,"indent":0,"parameters":[" but gave Tamaki and her mother the same love as ever.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Thus far, no tragedy had occurred.\""]},{"code":231,"indent":0,"parameters":[5,"mishiro_06",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...\""]},{"code":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"But when Tamaki turned 18,"]},{"code":401,"indent":0,"parameters":[" Rinzou made an unforgivable mistake.\""]},{"code":230,"indent":0,"parameters":[30]},{"code":249,"indent":0,"parameters":[{"name":"shock1","pan":0,"pitch":100,"volume":100}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\\c[2]\"...He had an liaison with Tamaki,"]},{"code":401,"indent":0,"parameters":[" his own daughter, and impregnated her.\""]},{"code":231,"indent":0,"parameters":[5,"mishiro_02",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...?!\""]},{"code":231,"indent":0,"parameters":[5,"yuzu_11",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...!\""]},{"code":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki was deeply wounded, and feared Rinzou."]},{"code":401,"indent":0,"parameters":[" And with her mother, she fled from him.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"But the child she bore had done no wrong,"]},{"code":401,"indent":0,"parameters":[" so she decided to birth the child of her father.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"So Tamaki had a daughter.\""]},{"code":231,"indent":0,"parameters":[5,"mishiro_02",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...A daughter between parent and child...\""]},{"code":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki thought she couldn't live a normal, happy life."]},{"code":401,"indent":0,"parameters":[" But luckily, two years later, she had a proper marriage.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Safely having a son with her husband,"]},{"code":401,"indent":0,"parameters":[" Tamaki began to get back on her feet.\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...However, she never fully did.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"The mental wounds Tamaki had suffered"]},{"code":401,"indent":0,"parameters":[" went even deeper than she imagined.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"When her now-grown daughter with Rinzou"]},{"code":401,"indent":0,"parameters":[" began doing art at school, Tamaki's old wounds ached.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"It gave her flashbacks of Rinzou,"]},{"code":401,"indent":0,"parameters":[" of that fear she had of him.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki struggled to give her daughter affection. It took"]},{"code":401,"indent":0,"parameters":[" all she had not to take out negative emotions on her.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Tamaki's mind quickly crumbled under the weight.\""]},{"code":231,"indent":0,"parameters":[5,"yuzu_11",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...\""]},{"code":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\\c[2]\"And one day, Tamaki killed her daughter.\""]},{"code":231,"indent":0,"parameters":[5,"mishiro_02",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...!\""]},{"code":231,"indent":0,"parameters":[5,"yuzu_12",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"..........\""]},{"code":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki fell to pieces. She came to feel animosity"]},{"code":401,"indent":0,"parameters":[" not only toward Rinzou, but all the Usuis.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Unable to see any hope ahead,"]},{"code":401,"indent":0,"parameters":[" she fell upon the despair behind.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Rinzou died of natural causes a few years later,"]},{"code":401,"indent":0,"parameters":[" and while she had the right to his inheritance...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"She refused to take it."]},{"code":401,"indent":0,"parameters":[" She wouldn't even let it go to the eldest son, Shinji.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Then at last, Tamaki, who had kept living"]},{"code":401,"indent":0,"parameters":[" despite having broken long ago, met her end.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...When she learned the son she had with her husband"]},{"code":401,"indent":0,"parameters":[" was becoming friends with Shinji's daughter.\""]},{"code":231,"indent":0,"parameters":[5,"mishiro_02",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...You mean...\""]},{"code":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki tried to take out Shinji's daughter."]},{"code":401,"indent":0,"parameters":[" The Usui who dared come near her pure son...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...But when she drove madly down the road to kill her,"]},{"code":401,"indent":0,"parameters":[" she ran over her own son by mistake.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki realized that this was the end.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"So she took her own life to escape the despair.\""]},{"code":231,"indent":0,"parameters":[5,"yuzu_13",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"No way... That's so... messed up...\""]},{"code":231,"indent":0,"parameters":[5,"tamaki_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...That woman's soul is now bound here."]},{"code":401,"indent":0,"parameters":[" Unlike the other souls, the world is forcing her to be here.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki's soul is now bound in death,"]},{"code":401,"indent":0,"parameters":[" so that she can't escape, but must face the facts...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...This is surely my punishment.\""]},{"code":205,"indent":0,"parameters":[1,{"list":[{"code":17,"indent":null,"parameters":[]},{"code":15,"indent":null,"parameters":[20]},{"code":16,"indent":null,"parameters":[]},{"code":0,"indent":null,"parameters":[]}],"repeat":false,"skippable":false,"wait":true}]},{"code":505,"indent":0,"parameters":[{"code":17,"indent":null,"parameters":[]}]},{"code":505,"indent":0,"parameters":[{"code":15,"indent":null,"parameters":[20]}]},{"code":505,"indent":0,"parameters":[{"code":16,"indent":null,"parameters":[]}]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...I've gone on for a while."]},{"code":401,"indent":0,"parameters":[" I can't see or hear you, but I know you're there.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"I don't know who you are, but go now.\""]},{"code":213,"indent":0,"parameters":[-1,8,false]},{"code":231,"indent":0,"parameters":[5,"mishiro_02",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-4