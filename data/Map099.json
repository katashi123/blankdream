{"id":1,"name":"EV001","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":204,"switch1Valid":true,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"","direction":2,"pattern":0,"characterIndex":0},"moveFrequency":3,"moveRoute":{"list":[{"code":0,"indent":null,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true,"list":[{"code":223,"indent":0,"parameters":[[0,0,0,51],1,true]},{"code":235,"indent":0,"parameters":[1]},{"code":230,"indent":0,"parameters":[60]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]After that, I regained consciousness in a hospital bed."]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]Apparently, I really had been saved,"]},{"code":401,"indent":0,"parameters":["and my soul returned safely from the Mirror World."]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]My mother was there,"]},{"code":401,"indent":0,"parameters":["looking at me in a way I felt she hadn't in years."]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]\"Mishiro...! You're awake...?!\""]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]\"Thank goodness... Oh, thank goodness...\""]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]\"If I had lost you, too...\""]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]\"I'm sorry, I'm sorry..."]},{"code":401,"indent":0,"parameters":[" I... I drove you to do this, didn't I...\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]\"...\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]My mother was shocked by my suicide attempt,"]},{"code":401,"indent":0,"parameters":["and apologized for her harsh discipline."]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]My head felt very hazy. I only remembered that"]},{"code":401,"indent":0,"parameters":["she was my mother, who became very strict at some point...."]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]It took a lot just to recognize that as real."]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]And as I thought about it, fear and unease"]},{"code":401,"indent":0,"parameters":["toward my mother came up from the gaps in my memory."]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]...But..."]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]\"...It's okay.\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]Ryotaro had told me about the pressure on the Usuis,"]},{"code":401,"indent":0,"parameters":["and how death had taxed my mother's mind..."]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["\\c[15]Now that I was able to accept that,"]},{"code":401,"indent":0,"parameters":["it was easy to hold my mother's hand."]},{"code":230,"indent":0,"parameters":[30]},{"code":223,"indent":0,"parameters":[[-255,-255,-255,0],90,true]},{"code":230,"indent":0,"parameters":[30]},{"code":231,"indent":0,"parameters":[1,"BG00c2_80",0,0,0,0,100,10