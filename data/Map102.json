{"id":1,"name":"EV001","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"","direction":2,"pattern":0,"characterIndex":0},"moveFrequency":3,"moveRoute":{"list":[{"code":0,"indent":null,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true,"list":[{"code":223,"indent":0,"parameters":[[0,0,0,51],60,true]},{"code":230,"indent":0,"parameters":[60]},{"code":117,"indent":0,"parameters":[2]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...All that talk about taking a trip together..."]},{"code":401,"indent":0,"parameters":[" So this was your goal.\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Trips are necessary for resting"]},{"code":401,"indent":0,"parameters":[" your body and mind, you know.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...And the kids seemed to have fun today too."]},{"code":401,"indent":0,"parameters":[" They're snoozing away now.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"A good thing for you, isn't it?\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...So, Tamaki. Won't you consider it?\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Drop it, please. Utsuro is my child.\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"But Seiichi's told me how you've been"]},{"code":401,"indent":0,"parameters":[" seeing the shadow of your father in Utsuro lately.\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Seiichi, you told him that?\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Sorry. But you've looked so frayed lately...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"He knows the situation,"]},{"code":401,"indent":0,"parameters":[" so I just thought I should talk to him about it.\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Tamaki, the biggest load on your mind..."]},{"code":401,"indent":0,"parameters":[" It's \"art,\" isn't it?\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Utsuro is 8 now... From now on,"]},{"code":401,"indent":0,"parameters":[" she's going to be drawing more at school.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...How about leaving Utsuro with your mother"]},{"code":401,"indent":0,"parameters":[" until she's old enough to take this calmly?\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"It'll be fine."]},{"code":401,"indent":0,"parameters":[" If I, her mother, can't protect her, then who will?\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"I suppose you're right, Tamaki..."]},{"code":401,"indent":0,"parameters":[" But it feels like you'll collapse at this rate.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"What'll happen to Ayato then?\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Nobody's perfect."]},{"code":401,"indent":0,"parameters":[" You don't need to carry everything alone.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Especially considering your burdens"]},{"code":401,"indent":0,"parameters":[" are much heavier than most.\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...But still...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"I still want to hold out..."]},{"code":401,"indent":0,"parameters":[" I don't want to break up this family of four...\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Tamaki...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Shinji, Chika. I'll support Tamaki as much as I can."]},{"code":401,"indent":0,"parameters":[" Let me try just a little longer.\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"Seiichi...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"If the two of you insist, then I won't stop you...\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"But just keep in mind, Tamaki."]},{"code":401,"indent":0,"parameters":[" You've already put in plenty of toil.\""]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"So at least stop before it does you in...\""]},{"code":230,"indent":0,"parameters":[30]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"...Right...\""]},{"code":117,"indent":0,"parameters":[3]},{"code":242,"indent":0,"parameters":[3]},{"code":234,"indent":0,"parameters":[1,[0,0,0,0],180,false]},{"code":223,"indent":0,"parameters":[[-255,-255,-255,51],180,true]},{"code":230,"indent":0,"parameters":[60]},{"code":235,"indent":0,"parameters":[5]},{"code":121,"indent":0,"parameters":[216,216,0]},{"code":201,"indent":0,"parameters":[0,101,11,10,8,0]},{"code":0,"indent":0,"parameters":[]}]}],"x":9,"y":7}
