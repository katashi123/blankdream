{"id":2,"name":"EV002","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"$SUIT_M02","direction":2,"pattern":1,"characterIndex":0},"moveFrequency":3,"moveRoute":{"list":[{"code":0,"indent":null,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":0,"walkAnime":true,"list":[{"code":117,"indent":0,"parameters":[2]},{"code":231,"indent":0,"parameters":[5,"ryotaro_02",0,0,-40,160,100,100,0,0]},{"code":232,"indent":0,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":["\"She's really stubborn, isn't she? If it were me,"]},{"code":401,"indent":0,"parameters":[" I'd have thrown in the towel after all that chasing...\""]},{"code":102,"indent":0,"parameters":[["Listen to Ryotaro","Nothing"],1,0,2,0]},{"code":402,"indent":0,"parameters":[0,"Listen to Ryotaro"]},{"code":231,"indent":1,"parameters":[5,"ryotaro_01",0,0,-40,160,100,100,255,0]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Got it. All right, let's begin.\""]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,0,0,10,false]},{"code":117,"indent":1,"parameters":[3]},{"code":246,"indent":1,"parameters":[3]},{"code":223,"indent":1,"parameters":[[-255,-255,-255,51],180,true]},{"code":230,"indent":1,"parameters":[60]},{"code":201,"indent":1,"parameters":[0,106,14,14,8,2]},{"code":205,"indent":1,"parameters":[0,{"list":[{"code":16,"indent":null,"parameters":[]},{"code":0,"indent":null,"parameters":[]}],"repeat":false,"skippable":false,"wait":false}]},{"code":505,"indent":1,"parameters":[{"code":16,"indent":null,"parameters":[]}]},{"code":241,"indent":1,"parameters":[{"name":"Vanity","pan":0,"pitch":100,"volume":100}]},{"code":230,"indent":1,"parameters":[60]},{"code":223,"indent":1,"parameters":[[0,0,0,51],180,true]},{"code":230,"indent":1,"parameters":[60]},{"code":117,"indent":1,"parameters":[2]},{"code":231,"indent":1,"parameters":[5,"yuzu_05",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Did we outrun her?\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...Seems so. I don't think she's giving chase.\""]},{"code":231,"indent":1,"parameters":[5,"mishiro_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"I think the goal is close by...\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...I don't know what'll be up ahead."]},{"code":401,"indent":1,"parameters":[" So I just want to get this out.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"I'm Ryotaro Sengoku. Younger brother of Chika Usui,"]},{"code":401,"indent":1,"parameters":[" Mishiro's mother. And subordinate to her dad, Shinji Usui.\""]},{"code":231,"indent":1,"parameters":[5,"mishiro_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...Okay.\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"I knew about you a while back from a New Year's card"]},{"code":401,"indent":1,"parameters":[" sis sent me. To be honest, I found you cute.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Not exactly a romantic thing, of course..."]},{"code":401,"indent":1,"parameters":[" I was captivated by your latent beauty, I suppose.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"But when I asked my boss about you,"]},{"code":401,"indent":1,"parameters":[" I learned you'd been glum and moping as of late.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"And I know why. You injured someone in middle school,"]},{"code":401,"indent":1,"parameters":[" and sis kept you shackled ever since.\""]},{"code":213,"indent":1,"parameters":[-1,8,false]},{"code":231,"indent":1,"parameters":[5,"mishiro_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...That's right.\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"My boss invited me over to your house,"]},{"code":401,"indent":1,"parameters":[" and I was honestly pumped to try and help you.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"It made me sad to imagine"]},{"code":401,"indent":1,"parameters":[" such a beautiful girl moping around...\""]},{"code":231,"indent":1,"parameters":[5,"yuzu_04",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"He says without a second of hesitation...\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_04",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"No poking fun, now. It's the end.\""]},{"code":231,"indent":1,"parameters":[5,"yuzu_11",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...Huh?\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_03",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"But I died before it happened."]},{"code":401,"indent":1,"parameters":[" We got caught in a fire in a building at work.\""]},{"code":231,"indent":1,"parameters":[5,"mishiro_06",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"A fire...\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_03",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Yes, I was with your father."]},{"code":401,"indent":1,"parameters":[" After that, well, it even kind of surprised me...\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_04",0,0,-40,160,100,100,255,0]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...Partly by will, I became a soul,"]},{"code":401,"indent":1,"parameters":[" and flew around to where you were... apparently.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"So for the year-or-so between my death and yours,"]},{"code":401,"indent":1,"parameters":[" my spirit was sort of haunting you.\""]},{"code":231,"indent":1,"parameters":[5,"mishiro_05",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Wha...\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_04b",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":205,"indent":1,"parameters":[2,{"list":[{"code":14,"indent":null,"parameters":[0,0]},{"code":0,"indent":null,"parameters":[]}],"repeat":false,"skippable":false,"wait":false}]},{"code":505,"indent":1,"parameters":[{"code":14,"indent":null,"parameters":[0,0]}]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Oh, but I was hardly even conscious of anything!"]},{"code":401,"indent":1,"parameters":[" I just vaguely know that's what happened!\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_03",0,0,-40,160,100,100,255,0]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...Ahem. For you see, ever since I saw your photo,"]},{"code":401,"indent":1,"parameters":[" I considered you someone beautiful and worth protecting.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"So even through death, I had the wish"]},{"code":401,"indent":1,"parameters":[" of getting to talk to you at least once.\""]},{"code":231,"indent":1,"parameters":[5,"mishiro_01",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"So that was your wish...\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_04",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"So it was. ...Thus, it's been granted.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"By following you, I wound up in this world,"]},{"code":401,"indent":1,"parameters":[" and in this world, my unrealized wish came true.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"In fact, the moment I became aware of my wish,"]},{"code":401,"indent":1,"parameters":[" I felt I could free myself from this world anytime.\""]},{"code":231,"indent":1,"parameters":[5,"mishiro_06",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...So you're going?\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_04",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...Well, I've come this far."]},{"code":401,"indent":1,"parameters":[" I'd like to see out your story to the end.\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_01",0,0,-40,160,100,100,255,0]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...Mishiro. The harsh circumstances you've had,"]},{"code":401,"indent":1,"parameters":[" and the fact that you're dead now...\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...I don't feel like it's irrelevant to me."]},{"code":401,"indent":1,"parameters":[" I can't say \"don't blame your parents.\"\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"But I just want you to know. I can't force you"]},{"code":401,"indent":1,"parameters":[" to understand or sympathize, but just know.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Your father worried over and regretted"]},{"code":401,"indent":1,"parameters":[" how he was spending less time with you.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"He wasn't just weighing"]},{"code":401,"indent":1,"parameters":[" you and the inheritance on a scale.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Your mother was working with your father"]},{"code":401,"indent":1,"parameters":[" to obtain \"proof\" that the Usui fortune belonged to them.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Maybe she failed to control her strength properly."]},{"code":401,"indent":1,"parameters":[" Maybe she wasn't calm. But she had no ill will, I'm sure.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"I'm sure after the boss and I died, she endured the sorrow"]},{"code":401,"indent":1,"parameters":[" of losing a husband and brother and fought for her family.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"Your father and mother might have been clumsy..."]},{"code":401,"indent":1,"parameters":[" But they weren't cold-blooded.\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"I just want you to know that.\""]},{"code":231,"indent":1,"parameters":[5,"mishiro_06",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_04",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"It's probably too late to say now, though...\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...Let's go on ahead.\""]},{"code":231,"indent":1,"parameters":[5,"ryotaro_01",0,0,-40,160,100,100,255,0]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"This is a door you're able to open."]},{"code":401,"indent":1,"parameters":[" I'm sure there's something important to you here.\""]},{"code":231,"indent":1,"parameters":[5,"mishiro_06",0,0,-40,160,100,100,0,0]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,255,0,20,false]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["\"...\""]},{"code":101,"indent":1,"parameters":["",0,2,2]},{"code":401,"indent":1,"parameters":["(...If you're telling me that, then..."]},{"code":401,"indent":1,"parameters":[" Where should my wish to erase my existence go?)"]},{"code":242,"indent":1,"parameters":[4]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,0,0,10,false]},{"code":117,"indent":1,"parameters":[3]},{"code":230,"indent":1,"parameters":[60]},{"code":223,"indent":1,"parameters":[[-255,-255,-255,51],180,true]},{"code":230,"indent":1,"parameters":[120]},{"code":245,"indent":1,"parameters":[{"name":"Darkness","pan":0,"pitch":100,"volume":60}]},{"code":201,"indent":1,"parameters":[0,106,14,14,8,0]},{"code":223,"indent":1,"parameters":[[0,0,0,51],60,true]},{"code":121,"indent":1,"parameters":[225,225,0]},{"code":0,"indent":1,"parameters":[]},{"code":402,"indent":0,"parameters":[1,"Nothing"]},{"code":232,"indent":1,"parameters":[5,0,0,0,-40,160,100,100,0,0,10,false]},{"code":117,"indent":1,"parameters":[3]},{"c